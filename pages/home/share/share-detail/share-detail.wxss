.objInfo{
  margin-top: 20px;
}

.warning{
  margin-left: 5rpx;
  margin-right: 5rpx;
}


.bottom-bar {
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  border-top: solid 1rpx #ddd;
}

.totalPrice {
  background-color: rgb(252, 252, 252);
  width: 50%;
  text-align: center;
}

.totalPrice text {
  position: relative;
  top: 18rpx;
}

.confirmOrder {
  background-color: red;
  width: 50%;
  text-align: center;
}

.confirmOrder text {
  position: relative;
  top: 18rpx;
  color: white;
}
